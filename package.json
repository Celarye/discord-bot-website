{
  "name": "discord-bot-website",
  "description": "The website to control and monitor the discord bot.",
  "private": true,
  "type": "module",
  "scripts": {
    "vue-tsc": "vue-tsc --project tsconfig.json --noEmit",
    "lint": "eslint --fix --cache --cache-location 'node_modules/.cache/eslint/.eslint-cache'",
    "lint:check": "eslint --cache --cache-location 'node_modules/.cache/eslint/.eslint-cache'",
    "format": "prettier --write '**/*{.md,.yml,.yaml,.json,.js,.ts,.vue}' --cache",
    "format:check": "prettier --check '**/*{.md,.yml,.yaml,.json,.js,.ts,.vue}' --cache",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "test": "pnpm run vue-tsc && pnpm run lint:check && pnpm run format:check && pnpm run build",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "nuxt": "^3.17.4",
    "vue": "^3.5.15",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@iconify-json/radix-icons": "^1.2.2",
    "@iconify/vue": "^5.0.0",
    "@nuxt/eslint": "1.4.1",
    "@nuxtjs/color-mode": "^3.5.2",
    "@tailwindcss/vite": "^4.1.7",
    "@vueuse/core": "^13.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.27.0",
    "js-yaml": "^4.1.0",
    "lucide-vue-next": "^0.511.0",
    "prettier": "^3.5.3",
    "reka-ui": "^2.2.1",
    "shadcn-nuxt": "2.1.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.7",
    "tw-animate-css": "^1.3.0",
    "typescript": "^5.8.3",
    "vue-tsc": "3.0.0-alpha.8"
  },
  "packageManager": "pnpm@10.11.0"
}
