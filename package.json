    "shadcn-nuxt": "2.1.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4",
    "tw-animate-css": "^1.2.8",
    "typescript": "^5.8.3",
    "vue-tsc": "^2.2.10",
    "yaml": "^2.8.0"
