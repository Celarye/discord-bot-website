{
  "name": "discord-bot-website",
  "description": "The website to control and monitor the discord bot.",
  "private": true,
  "type": "module",
  "scripts": {
    "vue-tsc": "vue-tsc --project tsconfig.json --noEmit",
    "lint": "eslint --fix --cache --cache-location 'node_modules/.cache/eslint/.eslint-cache'",
    "lint:check": "eslint --cache --cache-location 'node_modules/.cache/eslint/.eslint-cache'",
    "format": "prettier --write '**/*{.md,.yml,.yaml,.json,.js,.ts,.vue}' --cache",
    "format:check": "prettier --check '**/*{.md,.yml,.yaml,.json,.js,.ts,.vue}' --cache",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "test": "pnpm run vue-tsc && pnpm run lint:check && pnpm run format:check && pnpm run build",
    "postinstall": "nuxt prepare",
    "prepare": "husky"
  },
  "dependencies": {
    "nuxt": "^3.17.5",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
    "@iconify-json/radix-icons": "^1.2.2",
    "@iconify/vue": "^5.0.0",
    "@internationalized/date": "^3.8.2",
    "@nuxt/eslint": "1.6.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@pinia/nuxt": "^0.11.2",
    "@tailwindcss/vite": "^4.1.11",
    "@vueuse/core": "^13.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.31.0",
    "husky": "^9.1.7",
    "lucide-vue-next": "^0.525.0",
    "prettier": "^3.6.2",
    "reka-ui": "^2.3.2",
    "shadcn-nuxt": "2.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3",
    "vue-tsc": "^2.2.10",
    "yaml": "^2.8.0"
  },
  "packageManager": "pnpm@10.12.1"
}
